# ===========================================
# Apollo Panel Environment Configuration
# ===========================================
# Copy this file to .env and update with your actual values
# NEVER commit .env files to version control

# API Configuration
VITE_APP_API_SERVER=localhost:8000
VITE_APP_AUTHENTICATION_API_SERVER=localhost:8080
VITE_APP_FILE_SERVER=localhost:8080
VITE_APP_STATIC_SERVER=localhost:8080

# SSL Configuration
VITE_APP_SSL=false

# Development Configuration
NODE_ENV=development

# Optional: Analytics and Monitoring
# VITE_APP_ANALYTICS_ID=your-analytics-id
# VITE_APP_SENTRY_DSN=your-sentry-dsn

# Optional: Feature Flags
# VITE_APP_ENABLE_DEBUG_MODE=false
# VITE_APP_ENABLE_ANALYTICS=false
